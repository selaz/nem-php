{
    "name": "selaz/nem-php",
    "description": "Composer Package for PHP (Laravel or Pure PHP), Wrapper to use the NEM blockchain REST API and SDK with NIS compatibility.",
    "keywords": ["laravel", "framework", "nem", "cryptocurrency", "api", "sdk", "restful", "laravel-nem", "evias", "Gregory Saive", "nem-php", "php-nem"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gregory Saive",
            "email": "greg@evias.be"
        },
        {
            "name": "Robin Pedersen"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": ">5.0.33",
        "illuminate/support": ">5.0.33",
        "guzzlehttp/guzzle": ">5",
        "mdanter/ecc": "*",
        "paragonie/sodium_compat": "^1.4",
        "christian-riesen/base32": "^1.3",
        "kornrunner/keccak": "^1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "NEM\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
